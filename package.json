{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "dev:dynamic": "PORT=0 NODE_ENV=development tsx server/index.ts",
    "start:dynamic": "PORT=0 NODE_ENV=production node dist/index.js",
    "dev:smoke": "node scripts/dev-smoke-runner.mjs",
    "smoke:prod": "node scripts/smoke-runner.mjs",
    "lint": "eslint server scripts --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "tsc",
    "smoke": "npm run build && tsx scripts/smoke.ts",
    "e2e": "npm run build && tsx scripts/e2e.ts",
    "demo:screens": "npm run build && tsx scripts/e2e.ts",
    "nav:e2e": "npm run build && node --import tsx scripts/nav-e2e.ts",
    "browserslist:update": "npx update-browserslist-db@latest",
    "test": "npm run smoke",
    "test:comprehensive": "npm run build && tsx scripts/comprehensive-test.ts",
    "test:functional": "npm run build && tsx scripts/functional-tests.ts",
    "test:performance": "npm run build && tsx scripts/performance-tests.ts",
    "test:security": "npm run build && tsx scripts/security-tests.ts",
    "security:validate": "npm run build && tsx scripts/comprehensive-security-validation.ts",
    "test:deployment": "npm run build && tsx scripts/deployment-tests.ts",
    "test:e2e:enhanced": "npm run build && tsx scripts/enhanced-e2e.ts",
    "test:e2e:comprehensive": "npm run build && tsx scripts/comprehensive-e2e.ts",
    "validate-config": "tsx scripts/validate-config.ts",
    "run:func:timeout": "node scripts/run-with-timeout.mjs 600 'npm run test:functional'",
    "run:e2e:timeout": "node scripts/run-with-timeout.mjs 600 'npm run test:e2e:comprehensive'",
    "run:smoke:timeout": "node scripts/run-with-timeout.mjs 300 'npm run smoke'",
    "validate:performance": "tsx scripts/performance-validation-report.ts",
    "db:push": "drizzle-kit push",
    "test:workflow": "vitest run client/test/integration/workflow-research-log-api.test.ts client/test/integration/storage-research-log-seed.test.ts client/test/integration/workflow-memo-state-api.test.ts client/test/integration/workflow-normalization-state-api.test.ts client/test/integration/workflow-valuation-state-api.test.ts client/test/integration/workflow-monitoring-state-api.test.ts client/test/integration/workflow-presence-api.test.ts",
    "test:snapshots": "vitest run client/test/unit/snapshots.test.tsx client/test/unit/stage-snapshots.test.tsx",
    "test:snapshots:generate": "tsx scripts/generate-snapshots.ts"
  },
